{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-bitwise": false,
    "variable-name": [
      true, "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-snake-case"
    ],
    "indent": [true, "spaces", 2],
    "space-before-function-paren": true,
    "no-unused-expression": false,
    "no-string-literal": false
  },
  "rulesDirectory": []
}
